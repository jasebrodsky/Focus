{
    "functions": {
        "predeploy": [],
        "source": "functions"
    },
    "hosting": {
        "public": "web",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "appAssociation": "AUTO",
        "rewrites": [
            {
                "source": "**",
                "destination": "web/index.html"
            },
            {
                "source": "/links/**",
                "dynamicLinks": true
            }
        ]
    }
}
